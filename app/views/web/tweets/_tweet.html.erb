<div id="<%= dom_id tweet %>">
  <p>
    <strong>User:</strong>
    <%= tweet.user.name %>
  </p>

  <p>
    <strong>Username:</strong>
    <%= tweet.user.username %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= tweet.content %>
  </p>

 <p>
    <strong>Created at:</strong>
    <%= tweet.created_at %>
  </p>

  <p>
    <strong>Quote:</strong>
    <%= tweet.quote %>
  </p>

</div>
